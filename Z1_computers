package main

import (
	"fmt"
)

func Computers(n int) string {
	Digit := n % 10
	TwoDigits := n % 100

	if TwoDigits >= 11 && TwoDigits <= 19 {
		return fmt.Sprintf("%d компьютеров", n)
	} else if Digit == 1 {
		return fmt.Sprintf("%d компьютер", n)
	} else if Digit >= 2 && Digit <= 4 {
		return fmt.Sprintf("%d компьютера", n)
	} else {
		return fmt.Sprintf("%d компьютеров", n)
	}
}
